# Claude Docker + Graphiti Environment Configuration
# Copy this file to .env and fill in your values

# Neo4j Database Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# Claude Docker API Configuration
CLAUDE_DOCKER_URL=http://localhost:8000
# Number of parallel workers (optional, default: 3)
WORKERS=3

# Embeddings Configuration (choose one provider)
# Option 1: Google Gemini (recommended for Claude Docker)
GOOGLE_API_KEY=your-google-api-key-here

# Option 2: OpenAI
# OPENAI_API_KEY=your-openai-api-key-here

# Option 3: Voyage AI
# VOYAGE_API_KEY=your-voyage-api-key-here

# Optional: Neo4j Enterprise Features
USE_PARALLEL_RUNTIME=false

# Optional: Logging Level
LOG_LEVEL=INFO